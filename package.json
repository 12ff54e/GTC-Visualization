    "plotly.js": "^1.58.4",
    "transform-loader": "^0.2.4",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "ajv": "^7.2.1",
    "compression": "^1.7.4",
    "dotenv": "^8.2.0",
